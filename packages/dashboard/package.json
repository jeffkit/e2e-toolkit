{
  "name": "@preflight/dashboard",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n server,ui -c blue,green \"tsx watch server/index.ts\" \"vite\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:ui": "vite",
    "build": "tsc --noEmit && tsc -p tsconfig.server.json && vite build",
    "build:ui": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@preflight/core": "workspace:*",
    "@fastify/cors": "^10.0.0",
    "@fastify/static": "^8.3.0",
    "fastify": "^5.0.0",
    "js-yaml": "^4.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.0.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}
